{
  "name": "expo-with-pincode-root",
  "version": "0.3.11",
  "description": "Headless UI library for creating a pincode screen in your app.",
  "main": "package/build/index.js",
  "types": "package/build/index.d.ts",
  "repository": "https://github.com/sincerely-manny/expo-with-pincode",
  "bugs": {
    "url": "https://github.com/sincerely-manny/expo-with-pincode/issues"
  },
  "author": "Kirill Serebryannyy <sincerely.manny@gmail.com> (sincerely-manny)",
  "license": "MIT",
  "homepage": "https://github.com/sincerely-manny/expo-with-pincode#readme",
  "private": true,
  "keywords": ["react-native", "expo", "expo-with-pincode", "ExpoWithPincode"],
  "workspaces": ["package", "example"],
  "scripts": {
    "build": "cd package && expo-module build",
    "clean": "cd package && expo-module clean",
    "test": "cd package && expo-module test",
    "prepare": "cd package && expo-module prepare",
    "start": "cd example && expo start",
    "ios": "cd example && expo run:ios",
    "android": "cd example && expo run:android"
  },
  "dependencies": {
    "expo": "^52.0.11",
    "expo-linking": "^7.0.3",
    "expo-local-authentication": "*",
    "expo-router": "*",
    "expo-secure-store": "*",
    "expo-sqlite": "*",
    "jotai": "*",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "react-native-is-edge-to-edge": "^1.1.6",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "^4.3.0",
    "superjson": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/runtime": "^7.20.0",
    "@types/react": "~18.3.12",
    "babel-plugin-module-resolver": "^5.0.2",
    "typescript": "^5.3.3"
  }
}
